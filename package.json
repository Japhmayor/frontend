  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-react": "^7.0.1",
